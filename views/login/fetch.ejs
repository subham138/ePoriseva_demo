<%- include('../template/header'); %>
    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <div class="content-header">
            <div class="container-fluid">
                <div class="row mb-2">
                    <div class="col-sm-6">
                        <h1 class="m-0">Dashboard</h1>
                    </div><!-- /.col -->
                    <div class="col-sm-6">
                        <ol class="breadcrumb float-sm-right">
                            <li class="breadcrumb-item"><a href="#">Home</a></li>
                            <li class="breadcrumb-item active">Dashboard v1</li>
                        </ol>
                    </div><!-- /.col -->
                </div><!-- /.row -->
            </div><!-- /.container-fluid -->
        </div>
        <!-- /.content-header -->

        <!-- Main content -->
        <section class="content">
            <div class="container-fluid">
				<div class="row">
					<table width="100%" border="1">
						<tr>
							<th>Sl No</th>
							<th>Name</th>
							<th>Email</th>
							<th>Update</th>
							<th>Delete</th>
						</tr>
						<%
							if(userdata.length != 0){
								var i = 1;
								userdata.forEach((data) => {
						%>
						<tr>
							<td><%= i; %></td>
							<td><%= data.username; %></td>
							<td><%= data.email; %></td>
							<td><button type="button" class="btn btn-success"><a href="/update/<%= data.id; %>">Update</a></button></td>
							<td><button type="button" class="btn btn-danger"><a href="/update/<%= data.id; %>">Delete</a></button></td>
						</tr>
						<%
								i++;
								})
							}
						%>
					</table>
				</div>
            </div>
        </section>
        <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->
    <%- include('../template/footer'); %>